/*----------------------------------------*/
/* Modal
/*----------------------------------------*/

.modal-window {
  position: fixed;
  background-color: rgb(0 0 0 / 80%);
  top: 0;
  right: 0;
  bottom: 0;
  left: 0;
  z-index: 99999999;
  /*visibility: hidden;
  opacity: 0;*/
  transition: all 0.3s;
    overflow: hidden !important;
}
.modal-window:target {
  visibility: visible;
  opacity: 1;
  pointer-events: auto;
}
.modal-window > div {
    width: 50%;
    height:90%;
    position: absolute;
    top: 55%;
    left: 55%;
    transform: translate(-50%, -50%);
    padding: 2em;
    background: url('../../images/bg-modal/bg-01.png');
    background-repeat: no-repeat;
    -webkit-background-size: cover;
    background-size: 75%;
    object-fit: cover;
    z-index: 99999 !important;
}
.modal-window header {
  font-weight: bold;
}
.modal-window h1 {
  font-size: 150%;
  margin: 0 0 15px;
}

.modal-close {
  color: #aaa;
  line-height: 50px;
  font-size: 80%;
  position: absolute;
  right: 0;
  text-align: center;
  top: 0;
  width: 70px;
  text-decoration: none;
}
.modal-close:hover {
  color: black;
}

.modal-window .modal-name{
    font-size: 35px;
    font-weight: 700;
    position: relative;
    top: 165px;
    left: 68px;
    font-family: 'Balsamiq Sans', cursive;
    color: #5480af;
    width: 50%;
    text-align: center;
    line-height: 35px;
}

.modal-window input{
    position: absolute;
    top: 305px;
    left: 130px;
    border: none;
    height: 65px;
    width: 35%;
    z-index: 999;
    padding:20px;
    font-weight: 600;
    font-size: 17px;
}

.modal-window button{
    position: absolute;
    top: 410px;
    left: 180px;
    padding: 10px 40px 10px 40px;
    border: 1px solid #78f128;
    background: #78f128;
    font-weight: 600;
    color: white;
    border-radius: 5px;
    cursor:pointer;
}

