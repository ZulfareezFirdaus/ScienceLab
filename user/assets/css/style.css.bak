/*----------------------------------------*/
/* Folder
/*----------------------------------------*/

/* Default */

.folder {
    position: relative;
    top: 0;
    left: 0;
    background: #eee;
    width: 170px;
    height: 100px;
    box-sizing: border-box;
    border: 8px solid #F8D775;
    border-top-width: 32px;
    border-radius: 0px 5px 5px 5px;
    transition: border-top-width 0.3s, transform 0.2s;
    transform-origin: 0% 100%;
    cursor: pointer;
}

.folder:before,
.folder:after {
    content: '';
    background: #F8D775;
}

.folder:before,
.folder:after,
.folder i {
    position: absolute;
}

.folder:before {
    width: 53px;
    height: 11px;
    bottom: 90px;
    left: -8px;
    border-radius: 3px 3px 0px 0px;
}

.folder:after {
    width: 170px;
    height: 90px;
    line-height: 72px;
    text-align: center;
    bottom: -8px;
    left: -8px;
    transition: transform 0.2s, background 0.2s;
    transform-origin: 0% 100%;
}

.folder:after {
    background: #FFE9A2;
    border-bottom: 1px solid rgba(0, 0, 0, 0.15);
    border-radius: 0px 0px 5px 5px;
}

.folder i {
    z-index: 2;
    font-size: 45px;
    text-shadow: 0 1px 0 rgba(255, 255, 255, 0.25);
    text-align: center;
    bottom: 15px;
    left: 60px;
    transition: transform 0.2s, background 0.2s;
    transform-origin: 0% 100%;
    color: #F8D775;
}

.folder:hover {
    border-top-width: 8px;
}

.folder:hover:after,
.folder:hover i {
    transform: scaleY(0.85) skewX(-15deg);
}

.folder:hover:after {
    background: #ffe9a2;
}

.folder:hover:i {
    color: #4171a2;
}

.text-folder{
    padding: 5px 31px 30px 5px;
    cursor:pointer;
}

.text-folder .title{
    font-size: 15px;
    font-weight: 650;
    color:#3c4043;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 1;
    -webkit-box-orient: vertical;
    cursor:pointer;
}

.text-folder .file{
    font-size: 14px;
}

.bg-file-btn{
    background: white;
    border-radius: 16px;
    height: 44px;
    width: 33px;
}

/*----------------------------------------*/
/* Tooltip
/*----------------------------------------*/

.tooltips .tooltiptexts {
  visibility: hidden;
  width: 90%;
  background-color: #566e7a;
  color: #fff;
  text-align: center;
  border-radius: 6px;
  padding: 8px 8px;
  position: absolute;
  z-index: 999999999999999;
  top: 75%;
  left: 40%;
  margin-left: -60px;
}

.tooltips .tooltiptexts::after {
  content: "";
  position: absolute;
  bottom: 100%;
  left: 50%;
  margin-left: -5px;
  border-width: 5px;
  border-style: solid;
  border-color: transparent transparent #566e7a transparent;
}

.tooltips:hover .tooltiptexts {
  visibility: visible;
}

